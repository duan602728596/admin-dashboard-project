{
  "compilerOptions": {
    "alwaysStrict": true,
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "newLine": "LF",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "module": "NodeNext",
    "jsx": "react-jsxdev",
    "importHelpers": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}